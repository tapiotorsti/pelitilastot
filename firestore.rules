rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false; 
    
    match /user/{userId} {
    	allow read, write: if request.auth.uid == userId;
    
    match /item/{itemId} {
      allow read, write: if request.auth.uid == userId;
      }
      }
    }
  }
}